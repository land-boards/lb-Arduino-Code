//////////////////////////////////////////////////////////////////////////////
// void appendSD(void)
//////////////////////////////////////////////////////////////////////////////

void appendSD(void)
{
  String dataString = "";
  File dataFile = SD.open("datalog.txt", FILE_WRITE);
  dataString += "Adding Stuff";
  if (dataFile)
  {
    dataFile.println(dataString);
    dataFile.close();
  }
}

